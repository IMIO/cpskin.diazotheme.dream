<?xml version="1.0"?>
<object name="portal_css">

 <stylesheet title="cpskin.diazotheme.dream styles"
    id="++theme++cpskin.diazotheme.dream/css/styles.css"
    media="screen" rel="stylesheet" rendering="import"
    cacheable="True" compression="safe" cookable="True"
    enabled="1"
    expression="python:here.restrictedTraverse('@@currentTheme')() == 'cpskin.diazotheme.dream'"
    applyPrefix="True"
    insert-before="ploneCustom.css" />
 
 <stylesheet title="cpskin.diazotheme.dream mobile style"
    id="++theme++cpskin.diazotheme.dream/css/mobile-dream.css"
    media="handheld, screen and (max-device-width: 480px), screen and (max-width: 480px)"
    rel="stylesheet" rendering="import"
    cacheable="True" compression="safe" cookable="True"
    enabled="1" expression="request/HTTP_X_THEME_ENABLED | nothing"
    applyPrefix="True"
    insert-after="++theme++cpskin.diazotheme.dream/css/styles.css" />
   

</object>
